<div id="hotel-search">
  <h3 id="search-and-save-on-h">Search and Save on Hotels</h3>
  <form id="hotel-search-form" name="hotel-search-form" action="" method="post"
        class="label-font-overwrite input-font-overwrite">
    <div class="form-group">
      <label for="where-are-you-going-input">Where are you going</label>
      <input type="text" id="where-are-you-going-input" class="form-control"
             placeholder="City, Airport, Point of Interest, Hotel Name or U.S. Zip Code"/>
    </div>
    <div class="row">
      <div class="form-group col-md-3">
        <label id="check-in-label" for=check-in-input">Check-in </label>
        <input type="date" id="check-in-input" class="form-control"
               placeholder="Choose Date"/>
      </div>

      <div class="form-group col-md-3">
        <label id="check-out-label" for=check-out-input">Check-out </label>
        <input type="date" id="check-out-input" class="form-control"
               placeholder="Choose Date"/>
      </div>

      <div class="form-group col-md-3">
        <label id="rooms-label" for=rooms-input">Rooms </label>
        <select id="rooms-input" class="form-control">
          <option>1 Rooms</option>
          <option>2 Rooms</option>
          <option>3 Rooms</option>
          <option>4 Rooms</option>
          <option>5 Rooms</option>
          <option>6 Rooms</option>
          <option>7 Rooms</option>
          <option>8 Rooms</option>
          <option>9 Rooms</option>
          <option>10+ Rooms</option>
        </select>
      </div>
    </div>

    <div class="row">
      <div class="col-md-3">
        <button type="submit" id="hotel-search-form-submit" class="btn btn-success" value="submit">Search Hotels
        </button>
      </div>
      <div class="col-md-3">
        <button type="submit" id="hotel-search-form-submit" class="btn btn-primary" value="submit">I'm Feeling...
        </button>
      </div>
    </div>
  </form>

</div>
